<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="ABC" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
  	 <Logger name="pack2" level="trace">
      <AppenderRef ref="ABC"/>
    </Logger>
    <Root level="error">
      <AppenderRef ref="ABC"/>
    </Root>
  </Loggers>
</Configuration>
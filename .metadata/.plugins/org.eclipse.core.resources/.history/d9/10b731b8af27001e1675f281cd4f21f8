package com.reactiveworks.cxf.example;

import javax.jws.WebService;

@WebService(targetNamespace = "http://example.cxf.reactiveworks.com/", portName = "HelloPort", serviceName = "HelloService")
public class Hello {
	
	public String sayHello(String name) {
		return "Hello From " + name;
	}
}
// pata na bhai dekho kab tak chalega, aur bharosa hai nhi ki paisa lekr 
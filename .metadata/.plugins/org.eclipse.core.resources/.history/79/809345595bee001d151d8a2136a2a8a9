package Java8;
class Student1{
	String name;
	int rollno;
	int marks;
	int age;
	public Student1(String name, int rollno, int marks, int age) {
		super();
		this.name = name;
		this.rollno = rollno;
		this.marks = marks;
		this.age = age;
	}
}

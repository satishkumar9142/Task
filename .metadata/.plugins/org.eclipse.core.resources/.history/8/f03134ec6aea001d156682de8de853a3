package pack3;

import java.io.Externalizable;

class ExternalizableDemo implements Externalizable{
	String s;
	int i;
	int j;
	public ExternalizableDemo() {
		System.out.println("Public no arg constructor");
	}
	
	public ExternalizableDemo(String s, int i, int j){
		this.s =s;
		this.i=i;
		this.j =j;
	}
}
public class Q10 {
	

}
